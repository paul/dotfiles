" Font
"set guifont=EnvyCodeR:h12
"set noantialias
set guifont=Anonymous\ Pro\ for\ Powerline:h14

" No audible bell
set vb

" No toolbar
set guioptions-=T
" no scrollbars
set guioptions-=r
set guioptions-=R
set guioptions-=l
set guioptions-=L
set guioptions-=b

" Clipboard when visual select
set guioptions+=a

" Use console dialogs
set guioptions+=c

"hi normal guibg=black
"set transp=5

